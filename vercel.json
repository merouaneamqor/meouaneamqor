{
    "buildCommand": "pnpm run build",
    "outputDirectory": ".output",
    "framework": "nuxtjs",
    "builds": [
      {
        "src": "nuxt.config.ts",
        "use": "@nuxtjs/vercel-builder"
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": ".output/server/index.mjs"
      }
    ],
    "env": {
      "NITRO_PRESET": "vercel"
    }
  }